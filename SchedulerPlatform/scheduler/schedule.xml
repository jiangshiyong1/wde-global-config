<?xml version="1.0" encoding="utf-8"?>
<Scheduler>
  <!--调度信息配置-->
  <ScheduleConfig>
    <Strategy Name="G2Strategy" ScheduleFreq="10">
      <Priority>
        <Start>1</Start>
        <End>10</End>
      </Priority>
      <Queue>
        <MaxItem>100000000</MaxItem>
        <BlockSize>1000</BlockSize>
      </Queue>
      <!--调度器启动、注册-->
      <ScheduleInfo>
        <ZkHost>10.61.2.129:2181</ZkHost>
        <ZkRoot>/NEW_WDE</ZkRoot>
        <StartPort>23243</StartPort>
      </ScheduleInfo>
       <!--定时触发表达式解析策略-->
      <TriggerResolve>
        <Host>10.61.1.24</Host>
        <Port>19050</Port>
      </TriggerResolve>
    </Strategy>
  </ScheduleConfig>

  <!--调度信息库表配置-->
  <TaskScheduleInfo>
      <SQL>
        <IP>10.61.1.24</IP><Port>3306</Port>
        <User>wde</User><Password>ictsoft</Password>
      </SQL>
      <DB>wde_monitor_wm</DB><Table>schedule_info</Table>
      <NoticeSize>1000</NoticeSize>
  </TaskScheduleInfo>

  <!-- 任务表配置 -->
  <TaskInfo FlushFreq="30">
      <SQL>
        <IP>10.61.1.24</IP>
        <Port>3306</Port>
        <User>wde</User>
        <Password>ictsoft</Password>
      </SQL>
      <DB>wde_monitor_wm</DB><Table>schedule_task</Table>
      <NoticeSize>1000</NoticeSize>
  </TaskInfo>

  <!--采集节点遍历配置-->
  <ServerInfo FlushFreq="30">
    <ZkHost>10.61.2.129:2181</ZkHost>
    <ZkRoot>/NEW_WDE</ZkRoot>
  </ServerInfo>
</Scheduler>
